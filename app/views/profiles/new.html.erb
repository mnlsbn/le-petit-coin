 <div class='form'>
  <fieldset>
      <legend>Create your profile</legend>
        <div class="col-xs-12 col-sm-6 col-md-6">
            <div class="card-new text-center">
              <%= simple_form_for [@profile] do |f| %>
                <%= f.input :first_name %>
                <%= f.input :last_name %>
                <%= f.input :phone_number %>
                <%= f.input :picture %>
              <% end %>
            </div>
        </div>
  </fieldset>
</div>
