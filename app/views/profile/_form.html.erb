<%= simple_form_for(@profile) do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>

  <%= f.label :photo %>
  <%= f.text_field :photo %>

  <%= f.submit %>
<% end %>
